   "source": [
    "def sumcum(x):\n",
    "    total = x[-1]\n",
    "    for i in range(1, len(x)):\n",
    "        total += x[len(x) - 1 -i]\n",
    "        x[len(x) - 1 -i] = total\n",
    "    return x\n",
    "\n",
    "        \n",
    "        "
   ]
